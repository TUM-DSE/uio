config APPSQLITE
	bool
	default y

if APPSQLITE
	config APPSQLITE_TRACING
		bool "Enable tracing"
		select LIBUBPF
		select LIBUBPF_TRACER
		select LIBUSHELL_BPF
		default n

	config APPSQLITE_TRACING_NOMCOUNT
		bool "Enable tracing w/o mcount"
		select LIBUBPF
		select LIBUBPF_TRACER
		select LIBUSHELL_BPF
		default n
endif
